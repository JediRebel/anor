{
  "compilerOptions": {
    "target": "ES2020",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowJs": false,
    "noImplicitAny": true,
    "baseUrl": ".",
    "types": ["node"],
    "moduleDetection": "force"
  }
}
